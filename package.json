{
  "name": "openjam",
  "version": "0.4.0",
  "engines": {
    "node": "10.9.0"
  },
  "private": true,
  "homepage": "https://beta.openjam.eu",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
